<script src="https://maps.googleapis.com/maps/api/js?key="></script>
<script src="https://unpkg.com/@googlemaps/markerclustererplus/dist/index.min.js"></script>
<div class="container-fluid content-float">
  <div class="row content-float">
    <!-- SEARCH PANEL -->
    <div class="col-4 col-lg-4 col-md-4 col-sm-12 col-12" id="left">
      <div class="card mb-4 shadow-sm">
        <div class="card-body">
          <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
        </div>
      </div>
      <%= react_component("SearchResults", prerender: false) %>
    </div>

    <!-- GOOGLE MAP WILL BE HERE-->
    <div class="col-8 col-lg-8 col-md-8 hidden-xs">
      <div id="map" style="width:100%; height:100%;"></div>
    </div>
  </div>
</div>
